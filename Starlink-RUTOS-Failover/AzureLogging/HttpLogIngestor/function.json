{
  "bindings": [
    {
      "authLevel": "function",
      "type": "httpTrigger",
      "direction": "in",
      "name": "Request",
      "methods": ["post"]
    },
    {
      "type": "http",
      "direction": "out",
      "name": "Response"
    },
    {
      "type": "blob",
      "direction": "out",
      "name": "outputBlob",
      "path": "logs/router-{DateTime:yyyy-MM-dd}.log",
      "connection": "AzureWebJobsStorage"
    }
  ]
}
